{
    "name": "erwanastro/geojson-parcelles",
    "description": "Minimal Symfony 8 console application for processing cadastral parcels",
    "license": "mit",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/yaml": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
